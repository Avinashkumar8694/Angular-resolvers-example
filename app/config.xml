    <!-- <plugin name="cordova-plugin-neushake" spec="1.0.0" /> -->
    <plugin name="cordova-plugin-uniquedeviceid" spec="1.3.2"/>
    <plugin name="com.cordova.plugins.cookiemaster" spec="1.0.1"/>
    <plugin name="@ahovakimyan/cordova-plugin-wkwebviewxhrfix" spec="1.0.0"/>
    <plugin name="cordova-plugin-neucookies" spec="1.0.0"/>
</widget>